{
  "name": "lambda-webhooks",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:ordermentum/lambda-webhooks.git",
  "author": "Ishmeet <arshusingh16@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/aws-lambda": "^8.10.33",
    "@types/node": "^12.7.9",
    "@types/qs": "^6.5.3",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "serverless": "^1.53.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15"
  },
  "scripts": {
    "build:clean": "rm -rf build",
    "build": "yarn build:clean && tsc --noEmit && babel -Dd build src --extensions \".js,.ts,.tsx\"",
    "lint": "./node_modules/.bin/eslint '**/*.{ts,js}'",
    "typecheck": "tsc --noEmit",
    "deploy": "yarn build && serverless deploy -v"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "yarn run eslint --fix --max-warnings 0 --no-ignore",
        "git add"
      ]
    },
    "ignore": [
      "build"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  }
}
